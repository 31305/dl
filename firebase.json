{
  "hosting": {
	"site": "mentic",
    "public": "./rs", 
    "rewrites": [ {
	    "source": "**",
   		"destination": "/5.html"
	} ],
    "headers": [
	   {
      "source": "*",
      "headers": [ 
	  {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
      },
	  {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
      },
	  {
          "key": "Accept-Ranges",
          "value": "bytes"
      }
	  ]
    } 
	]
  }
}




