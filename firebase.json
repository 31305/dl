{
  "hosting": {

    "public": "./", 
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
	  ".git/**",
	  ".*"
    ],
    "headers": [
	   {
      "source": "*",
      "headers": [ 
	  {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
      },
	  {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
      }
	  ]
    } 
	]
  }
}




